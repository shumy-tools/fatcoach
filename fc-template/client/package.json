{
  "name": "fc-template",
  "version": "1.0.0",
  "scripts": {
    "validate": "svelte-check",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --single",
    "start:dev": "sirv public --single --dev",
    "watch:tailwind": "postcss src/global.css -o public/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss src/global.css -o public/index.css"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^4.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tailwindcss": "^1.6.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@types/codemirror": "0.0.97",
    "axios": "^0.19.2",
    "codemirror": "^5.56.0",
    "sirv-cli": "^1.0.0"
  }
}
